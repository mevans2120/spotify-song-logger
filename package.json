{
  "name": "spotify-song-logger",
  "version": "1.0.0",
  "description": "Automated Spotify song logger that logs every song played for 30+ seconds to a Google Sheet with comprehensive metadata",
  "main": "api/log-spotify.js",
  "type": "module",
  "scripts": {
    "dev": "vercel dev",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:watch": "jest --watch",
    "test:spotify": "node scripts/test-local-logging.js",
    "test:sheets": "node scripts/init-sheets.js --dry-run",
    "lint": "echo 'Linting not configured yet'"
  },
  "keywords": [
    "spotify",
    "google-sheets",
    "logging",
    "automation",
    "music",
    "analytics",
    "serverless",
    "vercel"
  ],
  "author": "Michael Evans",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "googleapis": "^128.0.0",
    "@vercel/kv": "^1.0.1"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "vercel": "^32.5.0"
  }
}
